menu "charLCD" "Character LCD I2C display"

CHARLCD_ADDRESS:
    type hex
    nodefault
    menu "I2C Address"

CHARLCD_CHARS:
    type int
    nodefault
    menu "LCD character count" "Number of characters of display per line"

CHARLCD_LINES:
    type int
    nodefault
    menu "LCD lines count" "Number of lines of display"

endmenu
